{
  "name": "sevnt",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "watch": "webpack -d --watch",
    "admin_watch": "webpack -d --watch --config webpack.admin.js",
    "build": "env NODE_ENV=production webpack -p && babel lib -d server/bin",
    "web_build": "env NODE_ENV=production babel lib -d server/bin",
    "admin_build": "env NODE_ENV=production webpack -p --config webpack.admin.js",
    "server": "env DEBUG=app:'*' babel-node $NODE_DEBUG_OPTION index.server",
    "stop": "pm2 stop SevntBackend",
    "prod": "env NODE_ENV=production PORT=3030 pm2 start index.server.js --name SevntBackend"
  },
  "dependencies": {
    "bcrypt": "^1.0.1",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "debug": "2.2.0",
    "express": "^4.14.0",
    "jwt-simple": "^0.5.1",
    "moment": "2.14.1",
    "mongoose": "^4.5.8",
    "morgan": "^1.7.0",
    "node-fetch": "^1.6.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pug": "^2.0.0-beta3",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-react-native": "1.9.0",
    "babel-regenerator-runtime": "^6.5.0",
    "es6-promise": "^3.2.1",
    "file-loader": "^0.9.0",
    "react": "15.3.2",
    "react-datepicker": "^0.40.0",
    "react-dom": "^15.3.2",
    "react-native": "^0.37.0",
    "react-native-awesome-card-io": "^0.5.0",
    "react-native-aws3": "0.0.3",
    "react-native-camera": "git+https://github.com/lwansbrough/react-native-camera.git#154cb7aa584aaf827117260b34ce82fd32cf433a",
    "react-native-datepicker": "^1.4.3",
    "react-native-drawer": "^2.3.0",
    "react-native-fbsdk": "^0.4.0",
    "react-native-google-places-autocomplete": "^1.2.6",
    "react-native-image-crop-picker": "^0.10.9",
    "react-native-image-picker": "^0.24.1",
    "react-native-modal-picker": "0.0.16",
    "react-native-share": "^1.0.17",
    "react-native-swipe-list-view": "^0.3.1",
    "react-native-swiper": "^1.5.4",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-persist": "^4.0.0-beta1",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.1",
    "whatwg-fetch": "^1.0.0"
  }
}
